<table class="table">
    <tr>
        <th>Title</th>
        <th>Contact</th>
        <th>Owner</th>
        <th>Status</th>
        <th>Edit Election</th>
        <th>Edit Ballot</th>
        <th>Edit Voters</th>
        <th>Delete Election</th>
        <th>Duplicate?</th>
    </tr>

    <% @elections.each do |election| %>
        <tr>
            <td><%= link_to election.title, election %></td>
            <td><%= election.contact %></td>
            <td><%= election.owner %></td>
            <td><%= election.status %></td>
            <td><%= link_to 'Edit', edit_election_path(election) %></td>
            <td>Edit Ballot</td>
            <td>Edit Voters</td>
            <td><%= link_to 'Destroy', election, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            <td>Duplicate</td>
        </tr>
    <% end %>
</table>
<br />
<%= link_to 'New Election', new_election_path %>
